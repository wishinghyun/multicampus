				return result;
			}
			function printGuGu(dan){
				dan = parseInt(window.prompt("숫자 dan값을 입력하세요"));
				re = dan * i;
				document.write("<table>")
				for(i=1; i<=9; i++){
					document.write("<tr><td>"+dan+" * "+i+" = "+re+"</td></tr>"")
				}
				document.write("</table>")
			}
			
		</script>
