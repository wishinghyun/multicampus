<script type="text/javascript">
function calcNum(num1,num2){
	num1 = parseInt(window.prompt("숫자를 입력하세요"));
	num2 = parseInt(window.prompt("숫자를 입력하세요"));
	result = num1 + num2;
	alert("입력한 숫자의 합은=>"+result);
}
		function printGuGu(dan){
			dan = parseInt(window.prompt("숫자 dan값을 입력하세요"));
			result = dan * i;
			document.write("<table>")
			for(i=1; i<=9; i++){
				document.write("<tr><td>"+dan+" * "+i+" = "+result+"</td></tr>"")
			}
			document.write("</table>")
		}
		
	</script>
