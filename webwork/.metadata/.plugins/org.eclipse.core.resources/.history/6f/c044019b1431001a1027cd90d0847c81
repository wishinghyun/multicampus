			function calcNum(){
				num1 = 100;
				num2 = 1000;
				result = num1 + num2;
				return result;
			}
			function printGuGu(){
				dan = parseInt(window.prompt("단을 입력하세요"));
				document.write("<table>");
				for(i=1;i<=9;i++){
					document.write("<tr>");
					document.write("<td>"+dan+" * "+i+" = "+dan*i+"</td>");
					document.write("</tr>");
				}
				document.write("</table>");
			}
			
		</script>
