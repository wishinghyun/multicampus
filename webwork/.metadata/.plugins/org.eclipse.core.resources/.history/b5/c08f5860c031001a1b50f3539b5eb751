<body>
	<script type="text/javascript">
		today = new Date()
		myarr = new Array()
		myarr[0] = "요일정보:"
		myarr[1] = today.getDate()
		for(i=0;i<myarr.length;i++){
			document.write(myarr[i])
		}
		document.write("<h2>"+myarr.join(': ')+"</h2>")
		
	</script>
</body>
