<head>
	<meta charset="EUC-KR">
	<title>Insert title here</title>
</head>
<body>
	<script type="text/javascript">
		var jumsu;
		jumsu= 88;
		switch(jumsu/10)
		{
			case 10:
			case 9:
			case 8:
				document.write("수");
				document.write("<h1>환영합니다(A)</h1>");
				document.write("<img src='../images/jang1.jpg'/>");
				break;
			case 7:
			case 6:
				document.write("우");
				break;
			case 5:
			case 4:
				document.write("미");
				break;
			case 3:
			case 2:
				document.write("양");
				break;
			case 1:
			case 0:
				document.write("가");
				break;
			default:
				alert("잘못입력");
		}
	</script>
</body>
</html>