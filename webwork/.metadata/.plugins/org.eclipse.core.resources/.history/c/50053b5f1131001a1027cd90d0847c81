<head>
	<meta charset="EUC-KR">
	<title>Insert title here</title>
	<script type="text/javascript">
		function printGuGu(dan){
			dan = parseInt(window.prompt("숫자 num1값을 입력하세요"));
			result = dan * i;
			for(i=1; i<=9; i++){
				document.write(dan+" * "+i+" = "+result)
			}
		}
		function calcNum(num1,num2){
			result = num1 + num2;
			return result;
		}
	</script>
</head>
<body>
	<input type="button" value="구구단 출력하기" onclick="printGuGu(dan)">
	<script type="text/javascript">
		result = calcNum(num1,num2);
		alert("입력한 숫자의 합은=>"+result);
	</script>
