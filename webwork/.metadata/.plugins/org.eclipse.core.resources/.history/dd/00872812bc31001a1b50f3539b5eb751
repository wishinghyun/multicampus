	<script type="text/javascript">
		today = new Date()
		document.write("<h2>"+today+"</h2>")
		dday = new Date()
		dday.setMonth(1)
		dday.setDate(3)
		document.write("<h2>"+dday+"</h2>")
		document.write("<h2>"+(dday-today)+"</h2>")
	</script>
